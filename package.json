  "scripts": {
    "dev": "hugo server --disableFastRender --gc",
    "dev:preview": "hugo server --disableFastRender --gc --buildDrafts --buildFuture",
    "build": "hugo --gc --minify --cleanDestinationDir",
    "build:preview": "hugo --gc --minify --cleanDestinationDir --buildDrafts --buildFuture",
    "renderToDisk:dev": "hugo server --disableFastRender --gc --renderToDisk --cleanDestinationDir",
    "renderToDisk:prod": "hugo server --disableFastRender --gc --renderToDisk --minify --cleanDestinationDir --environment production"    
  },
